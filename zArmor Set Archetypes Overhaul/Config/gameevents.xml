<!-- [mod] FREERUNNER: EVASIVE DASH EVENT -->
    <!-- This game event is called by the Freerunner set to teleport the player a short distance forward. -->
    <action_sequence name="action_freerunner_dash">
        <property name="allow_user_trigger" value="false" />
        <property name="action_type" value="Game" />

        <!-- Requirement: Player must not be in a vehicle. -->
        <requirement class="InVehicle">
            <property name="invert" value="true" />
        </requirement>

        <!-- The teleport action -->
        <action class="Teleport">
            <!-- X, Y, Z: Teleports the player 2 meters backward. -->
            <property name="target_position" value="0,1, -2" />
            <property name="offset_type" value="Relative" />
        </action>

        <!-- A subtle sound effect for feedback -->
        <action class="PlaySound">
            <property name="sound" value="swoosh" />
        </action>
    </action_sequence>
